                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
	x.PersonSer,
	x.DiagnosticSer,
	x.CourseSer,
	MAX(x.HstryDateTime) MaxLogDate
FROM (SELECT
	a.PatientSer PersonSer,
	a.DiagnosticSer,
	a.CourseSer,
	a.logdate HstryDateTime
FROM dbo.olLogging a
WHERE UPPER(a.action) LIKE '%DELETE%'
UNION
SELECT
	b.PersonSer,
	b.DiagnosticSer,
	b.CourseSer,
	b.HstryDateTime
FROM dbo.olCaDirSurg b
UNION
SELECT
	c.PersonSer,
	c.DiagnosticSer,
	c.CourseSer,
	c.HstryDateTime
FROM dbo.olChemotherapy c
UNION
SELECT
	d.PersonSer,
	d.DiagnosticSer,
	d.CourseSer,
	d.HstryDateTime
FROM dbo.olCourse d
UNION
SELECT
	e.PersonSer,
	e.DiagnosticSer,
	NULL CourseSer,
	e.HstryDateTime
FROM dbo.olDiagnostic e
UNION
SELECT
	f.PersonSer,
	f.DiagnosticSer,
	NULL CourseSer,
	f.HstryDateTime
FROM dbo.olDiagnosticFacility f
UNION
SELECT
	g.PersonSer,
	g.DiagnosticSer,
	g.CourseSer,
	g.HstryDateTime
FROM dbo.olHormone g
UNION
SELECT
	h.PersonSer,
	h.DiagnosticSer,
	h.CourseSer,
	h.HstryDateTime
FROM dbo.olImmunotherapy h
UNION
SELECT
	i.PersonSer,
	i.DiagnosticSer,
	i.CourseSer,
	i.HstryDateTime
FROM dbo.olNonCaDirSurg i
UNION
SELECT
	j.PersonSer,
	j.DiagnosticSer,
	j.CourseSer,
	j.HstryDateTime
FROM dbo.olOtherTherapy j
UNION
SELECT
	k.PersonSer,
	k.DiagnosticSer,
	k.CourseSer,
	k.HstryDateTime
FROM dbo.olVolume k) x
JOIN dbo.olDiagnostic d2
	ON x.DiagnosticSer = d2.DiagnosticSer
JOIN dbo.olDiagnosticFacility df
	ON d2.DiagnosticSer = df.DiagnosticSer
JOIN dbo.olPatientFacility pf
	ON d2.PersonSer = pf.PersonSer
JOIN dbo.olDiagnosticSup s
	ON d2.DiagnosticSer = s.DiagnosticSer
JOIN dbo.OlPatientFacility pf2
	ON s.PersonSer = pf2.Personser
WHERE LTRIM(RTRIM(REPLACE(REPLACE(s.String, CHAR(10), ''), CHAR(13), ''))) IS NOT NULL
AND s.Identifier LIKE 'OHSU%%'
AND pf2.Facility = 4444
AND df.ClassCase IN ('00', '10', '11', '12', '13', '14', '20', '21', '22')
AND pf.MedRecNum NOT LIKE 'T%'
GROUP BY	x.PersonSer,
			x.DiagnosticSer,
			x.CourseSer</property>
