              refId="Package\Data Flow Task\OLE DB Source"
              componentClassID="{165A526D-D5DE-47FF-96A6-F8274C19826B}"
              contactInfo="OLE DB Source;Microsoft Corporation;Microsoft SqlServer v9; (C) 2005 Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="OLE DB Source"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT

DISTINCT(server.server_name)  as 'Server Name', 
ISNULL(db_v.[db_name],'') as 'Database', 

MAX( ISNULL([server].server_status,'')) as 'Server Status',
RIGHT(MAX( ISNULL([server].server_description,'')), 250) as 'Server Description',
MAX( ISNULL(server_alias.server_alias,''))  as 'Server Alias',

MAX( ISNULL(db_alias.db_alias,''))  as 'Database Alias',
RIGHT(MAX( ISNULL(db_v.db_description,'')), 250)  as 'Database Description',
MAX( ISNULL(db_v.db_status,''))  as 'Database Status',


MAX( case when db_v.db_usage like '%Prod%' then 'Production' 
else ISNULL(db_v.db_usage,ISNULL([server].usage,'')) end)  as 'Database App Usage',


MAX( ISNULL([server].usage,ISNULL(db_v.db_usage,'')))  as 'Server Usage',
MAX( ISNULL(db_v.db_type,''))  as 'Database App Type',
MAX( ISNULL(db_v.[app_name],''))  as 'Database App Name',

MAX( ISNULL(db_v.db_version,''))  as 'Database App Version',

MAX(ISNULL(case when db_v.db_support_days = db_v.db_support_hours 
then db_v.db_support_days else db_v.db_support_days + ', ' + 
db_v.db_support_hours end,'') )  as 'Support Hours',

-------Server Contacts
'Frank Evans'  as Manager,
       
MAX(case s.contact_roles_id when 4 then upper(left(s.contact_email,
charindex('@',s.contact_email)-1)) else '' end)  as 'DBAD Primary',  
MAX(case s.contact_roles_id when 5 then upper(left(s.contact_email,
charindex('@',s.contact_email)-1)) else '' end)  as 'DBAD Secondary',
       
-------Database Contacts
MAX(case d.contact_roles_id when 1 then upper(left(d.contact_email,
charindex('@',d.contact_email)-1)) else '' end)  as 'Primary Contact',
MAX(case d.contact_roles_id when 2 then upper(left(d.contact_email,
charindex('@',d.contact_email)-1)) else '' end)  as 'Secondary Contact',
MAX(case d.contact_roles_id when 3 then upper(left(d.contact_email,
charindex('@',d.contact_email)-1)) else '' end)  as 'Data Steward',
MAX(case d.contact_roles_id when 6 then upper(left(d.contact_email,
charindex('@',d.contact_email)-1)) else '' end)  as 'ITG Contact',
MAX(case d.contact_roles_id when 7 then upper(left(d.contact_email,
charindex('@',d.contact_email)-1)) else '' end)  as 'Downtime Notify'
                 
FROM  [server] 
JOIN db_v ON [server].server_id = db_v.server_id 

left outer join server_alias on server_alias.server_id = [server].server_id
left outer join db_alias on db_alias.db_id = db_v.db_id

left outer JOIN contact_to_db_v d on db_v.db_id = d.db_id
left outer join contact_to_server_v s on server.server_id = s.server_id

WHERE db_v.[db_name] not in ('master','model','tempdb','msdb','AdventureWorks',
'AdventureWorks2008','AdventureWorksDW','OHSU_sqltab','ReportServer','ReportServerTempDB','sqltab','Northwind')

AND isnull(db_v.db_status,'X') not in ('Obsolete','Unsupported')
AND ( db_v.db_usage in ('Reporting','Prod and Dev','Production','Development','Training') OR db_v.db_usage is null )

GROUP BY server.server_name, db_v.[db_name]</property>
