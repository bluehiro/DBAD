              <ProjectItem
                Name="ScriptMain.vb"
                Encoding="UTF8"><![CDATA[' Microsoft SQL Server Integration Services Script Task
' Write scripts using Microsoft Visual Basic 2008.
' The ScriptMain is the entry point class of the script.

'Written by Vince James 20140916 for DBA PULL job to record drivespace history
'Changelog
'20141112 VMJ - Modified ScriptResults to always be success & added error logging in Catch Ex

Imports System
Imports System.Data
Imports System.Data.SqlClient
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime
Imports System.IO

<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
	Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

	Enum ScriptResults
		Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
        'Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
	End Enum
	
    Public Sub Main()

        Dim strSql As String
        Dim strResult As String
        Dim strRunDateTime As DateTime
        strRunDateTime = Dts.Variables("StartTime").Value
        Dts.Variables("ServerErrors").Value = ""


        Try
            strResult = Dts.Variables("SQLVersionNbr").Value.ToString()

            strSql = " CREATE TABLE #tbl_OHSU_CaptureFreeSpace (drive_letter VARCHAR(64),mb_free INT)" & _
                    " CREATE TABLE #tbl_OHSU_FinalFreeSpaceReport (servername VARCHAR(50),drive_letter CHAR(1),mb_free INT)" & _
                    " INSERT INTO #tbl_OHSU_CaptureFreeSpace EXEC master..xp_fixeddrives" & _
                    " INSERT INTO #tbl_OHSU_FinalFreeSpaceReport (servername, drive_letter, mb_free)" & _
                    " SELECT @@servername,	drive_letter,mb_free FROM #tbl_OHSU_CaptureFreeSpace" & _
                    " SELECT 	toffsr.servername, toffsr.drive_letter, toffsr.mb_free FROM #tbl_OHSU_FinalFreeSpaceReport toffsr" & _
                    " IF OBJECT_ID('tempdb..#tbl_OHSU_CaptureFreeSpace') IS NOT NULL	DROP TABLE #tbl_OHSU_CaptureFreeSpace" & _
                    " IF OBJECT_ID('tempdb..#tbl_OHSU_FinalFreeSpaceReport') IS NOT NULL	DROP TABLE #tbl_OHSU_FinalFreeSpaceReport"

            Dim conn As New SqlConnection("Server=" & Dts.Variables("SRV_Conn").Value.ToString & "; Trusted_Connection=True")
            conn.Open()
            Dim cmd As New SqlCommand(strSql, conn)
            Dim reader As SqlDataReader = cmd.ExecuteReader()

            If reader.HasRows Then
                Dim AdoConn As SqlClient.SqlConnection
                AdoConn = DirectCast(Dts.Connections("DBA_Rep").AcquireConnection(Dts.Transaction), SqlClient.SqlConnection)
                Dim cmdAdo As New SqlCommand()

                Do While reader.Read()
                    cmdAdo = New SqlCommand("INSERT INTO [DBA_Rep].[dbo].[DBA_PULL_drivespace_history] ([servername], [drive_letter], [mb_free], [retrievedate], [ps]) VALUES ('" & _
                                        reader.GetString(0) & "','" & reader.GetString(1) & _
                                        "','" & reader.GetInt32(2) & "','" & strRunDateTime & "', 0)", AdoConn)
                    cmdAdo.ExecuteNonQuery()
                Loop

                AdoConn.Close()
            End If

            conn.Close()

        Catch ex As Exception
            'MsgBox(ex.Message)

            'Dts.Variables("ServerErrors").Value = vbCrLf & Dts.Variables("ServerErrors").Value & _
            '" Missing Backup Failed: " & Dts.Variables("SRV_Conn").Value

            Dts.Variables("ServerErrors").Value = "ScriptTask Error: " & ex.Message.ToString

        End Try


        Dts.TaskResult = ScriptResults.Success
    End Sub

End Class]]></ProjectItem>
