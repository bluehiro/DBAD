      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{1104111B-1B96-422F-813B-54598ABB5064}"
          SQLTask:SqlStatementSource="--- Data Scrub ---&#xA;&#xA;-- Update BackupDevice --&#xA;Update DBA_Rep.dbo.Backup_History&#xA;Set BackupDevice = 'SQL Job - Ola Scripts'&#xA;where Physical_device_name like '\\ohsu.edu\share\APP\SQL%'&#xA;and BackupDevice is null&#xA;&#xA;Update DBA_Rep.dbo.Backup_History&#xA;Set BackupDevice = 'Avamar'&#xA;where Physical_device_name like '(local)%'&#xA;and BackupDevice is null&#xA;&#xA;Update DBA_Rep.dbo.Backup_History&#xA;Set BackupDevice = 'Other'&#xA;where BackupDevice is null&#xA;-- /Update BackupDevice --&#xA;&#xA;-- De-Dup --&#xA;Delete&#xA;From DBA_Rep.dbo.Backup_History&#xA;Where ID &gt; (&#xA;Select Min(ID)&#xA;From  DBA_Rep.dbo.Backup_History a&#xA;Where a.Server_name = DBA_Rep.dbo.Backup_History.Server_name&#xA;and a.Database_name = DBA_Rep.dbo.Backup_History.Database_name&#xA;and a.Backup_start_date = DBA_Rep.dbo.Backup_History.Backup_start_date&#xA;and a.Physical_device_name = DBA_Rep.dbo.Backup_History.Physical_device_name)&#xA;-- /De-Dup --&#xA;&#xA;--- /Data Scrub ---" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
