                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT 	CASE WHEN f.servergroup = 'sqlVLDB_prod' THEN 'Networker' ELSE 'Ola' END AS BACKUP_APP,
		f.database_name AS BACKUP_NAME,
		f.servergroup AS BACKUP_GROUP,
		'MSSQL' AS BACKUP_TYPE,
		'Missing' AS BACKUP_STATUS,
		f.server_name AS BACKUP_CLIENT,
		REPLACE((p.Server_OS + ' ' + ISNULL(p.Server_SP, '') + ' ' + p.Server_Version), '  ', ' ') AS CLIENT_OS,
		'Full' AS BACKUP_LEVEL,
		'0' AS BACKUP_ID,
		'http://sql2k12rpt1/ReportServer/Pages/ReportViewer.aspx?%2fDBA_Rep_Reports%2fITG_Backup_Admins%2fMSSQL_Backup_Logs&amp;ServerName=' + f.server_name + '&amp;databasename=' + f.database_name AS BACKUP_LOG,
		f.last_backup AS BACKUP_DATETIME,
		f.retrievedate as STATUS_RECORDED,
		'sql_concierge' AS RECORDED_BY
FROM DBA_PULL_last_backup_hist_fail f
LEFT JOIN DBA_PULL_server_properties p
ON p.[Server Name] = f.server_name
WHERE f.retrievedate &gt; (GETDATE() - 1)</property>
